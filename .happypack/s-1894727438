'use strict';

Object.defineProperty(exports, "__esModule", {
  value: true
});

require('bootstrap-notify');

var notify = function notify(level, message) {
  $.notify({
    message: message
  }, {
    type: level, //info,danger,warning,success
    delay: 5000,
    placement: {
      from: 'top',
      align: 'center'
    },
    offset: 20,
    z_index: 1051,
    timer: 1000
  });
};

exports.default = notify;